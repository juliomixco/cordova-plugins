<?xml version='1.0' encoding='utf-8'?>
<widget id="com.testap" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="BXLService">
        <param name="android-package" value="com.plugins.bixolonprint.BXLService" />
    </feature>
    <feature name="WSMPrintService">
        <param name="android-package" value="com.plugins.woosimprint.WSMPrintService" />
    </feature>
    <feature name="Printer">
        <param name="android-package" value="de.appplant.cordova.plugin.printer.Printer" />
    </feature>
    <feature name="BarcodeScanner">
        <param name="android-package" value="com.plugins.barcodeScanner.BarCodeScanner" />
    </feature>
    <allow-intent href="market:*" />
    <name>Plugin app</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-navigation href="*://*.google.com/*" />
    <allow-navigation href="*://*.pubnub.com/*" />
</widget>
